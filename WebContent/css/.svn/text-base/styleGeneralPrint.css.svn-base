* {
	margin: 0;
	padding: 0;
}

html {
	font-size: 50.01%;
}

body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1em;
	margin-top: 0;
	margin-left: 0;
	margin-right: 0;
	margin-bottom: 0;
}

.ocultar_print {
	display: none;
}
/************* INFORMACION DEL CLIENTE *************/
.info {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 10px;
	background-color: #ffffff;
}

.redText {
	color: #990000
}

/************ TABLAS ************/
.lista {
	font-size: 10px;
	color: #333399;
}

.row1 {
	background-color: #FFFFFF;
}

.row2 {
	background-color: #E6E6E6;
}

table.borde {
	border: solid;
	border-width: 1px;
	padding: 1;
	border-color: gray;
	border-spacing: 1px;
}

td.borde {
	border: solid;
	border-width: 1px;
	border-color: #000000;
	border-spacing: 0px;
}

th.borde {
	background-color: #114971;
	color: #FFFFFF;
	padding: 1;
	font-size: 10px;
	align: left;
}